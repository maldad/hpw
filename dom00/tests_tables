datos_filas = tabla_datos_cuerpo.children[0].children[0]

var select_rows = function(){
    var datos_filas = [];
  for(var i = 0; i < 10; i++){
    datos_filas.push(tabla_datos_cuerpo.children[i].children[0]);
  }
  return datos_filas;
}
select_rows()
