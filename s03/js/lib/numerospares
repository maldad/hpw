NumerosEnteros.desde_hasta(1, 10);
NumerosEnteros.desde_hasta(3, 9);

var es_par = function(numero) {
  return (numero % 2 === 0); //funcion que regresa true/false
};

Arreglo.filter(es_par, NumerosEnteros.desde_hasta(0, 100));

Arreglo.filter(function(numero) {
  return (numero % 2 === 0); //funcion que regresa true/false
}, NumerosEnteros.desde_hasta(3, 4));
